#include <iostream> 
#include <stdio.h>
#include <string.h>
#include <conio.h>

using namespace std; 

int main()
{
	int i, j, n; 
	char elem[30]; 
	
	cout << "Number of Arrays to be Sorted: ";
	cin >> n; 
	
	cout << "Enter Array Elements:\n";
	
	for (i=1; i<=n; i++)
	
		cin >> elem[i];
		
			for (i=1; i<=n-1; i++)
			
				for (j=i; j<=n; j++)
				
					if (elem[i] > elem[j]) 
					swap (elem[i], elem[j]);
				
	for (i=1; i<=n; i++)
	{
		cout << elem[i] << ","; 
	}
	
	
_getch();
return 0;
}
